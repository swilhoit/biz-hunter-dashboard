# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Scraping Configuration
SCRAPING_SCHEDULE_ENABLED=false
SCRAPING_CRON_EXPRESSION="0 2 * * *"
SCRAPING_MAX_PAGES=3
SCRAPING_DELAY_MS=3000
VITE_SCRAPING_API_URL=http://localhost:3001

# ScrapeGraph AI Configuration (Alternative AI-powered scraper)
VITE_SCRAPEGRAPH_API_KEY=your_scrapegraph_api_key_here
# Get your API key from: https://scrapegraphai.com/

# Traditional ScraperAPI Configuration (Fallback)
SCRAPER_API_KEY=your_scraper_api_key_here

# Amazon Analytics Integration
VITE_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_JUNGLE_SCOUT_API_KEY=your_jungle_scout_api_key_here
REACT_APP_JUNGLE_SCOUT_KEY_NAME=your_jungle_scout_key_name_here
VITE_JUNGLE_SCOUT_API_KEY=your_jungle_scout_api_key_here
VITE_JUNGLE_SCOUT_KEY_NAME=your_jungle_scout_key_name_here
# DataForSEO Configuration (Replaces Rainforest API)
DATAFORSEO_USERNAME=your_dataforseo_username_here
DATAFORSEO_PASSWORD=your_dataforseo_password_here
VITE_DATAFORSEO_USERNAME=your_dataforseo_username_here
VITE_DATAFORSEO_PASSWORD=your_dataforseo_password_here
REACT_APP_DATAFORSEO_USERNAME=your_dataforseo_username_here
REACT_APP_DATAFORSEO_PASSWORD=your_dataforseo_password_here

# Google Ads API Configuration
# To get these credentials:
# 1. Go to https://ads.google.com/aw/apicenter
# 2. Create a developer token
# 3. Set up OAuth2 credentials in Google Cloud Console
# 4. Use the OAuth2 playground to get a refresh token
GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id
GOOGLE_ADS_CLIENT_SECRET=your_google_ads_client_secret
GOOGLE_ADS_DEVELOPER_TOKEN=your_google_ads_developer_token
GOOGLE_ADS_REFRESH_TOKEN=your_google_ads_refresh_token
GOOGLE_ADS_CUSTOMER_ID=your_google_ads_customer_id_without_dashes

# Development
NODE_ENV=development